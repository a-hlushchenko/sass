				{{/with}}
			</div>
		</section>
		{{/with}}
	</div>
</main>

<footer class="footer">
	{{#each footer}}
	<div class="footer-list">
		<strong class="list-title">{{title}}</strong>
		<div class="links">
			{{#each links}}
			<a class="link" href="{{url}}">{{text}}</a>
			{{/each}}
		</div>
	</div>
	{{/each}}
</footer>